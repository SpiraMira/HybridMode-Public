<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>
	<dict>
		<key>id</key>
		<string>Email Address</string>
		<key>regularExpression</key>
		<string>\b[A-Za-z0-9._%-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,4}\b</string>
		<key>allowsBackreferences</key>
		<true/>
	</dict>
	<dict>
		<key>id</key>
		<string>Web Address</string>
		<key>regularExpression</key>
		<string>(?:[A-Za-z][A-Za-z0-9+.-]{1,120}:[A-Za-z0-9/](?:(?:[A-Za-z0-9$_.+!*,;/?:@&amp;~=-])|%[A-Fa-f0-9]{2}){1,333}(?:#(?:[a-zA-Z0-9][a-zA-Z0-9$_.+!*,;/?:@&amp;~=%-]{0,1000}))?)</string>
		<key>allowsBackreferences</key>
		<true/>
	</dict>
	<dict>
		<key>id</key>
		<string>Phone Number</string>
		<key>regularExpression</key>
		<string>(?:(?:\+?1\s*(?:[.-]\s*)?)?(?:\(\s*(?:[2-9]1[02-9]|[2-9][02-8]1|[2-9][02-8][02-9])\s*\)|(?:[2-9]1[02-9]|[2-9][02-8]1|[2-9][02-8][02-9]))\s*(?:[.-]\s*)?)?(?:[2-9]1[02-9]|[2-9][02-9]1|[2-9][02-9]{2})\s*(?:[.-]\s*)?(?:[0-9]{4})(?:\s*(?:#|x\.?|ext\.?|extension)\s*(?:\d+))?</string>
		<key>allowsBackreferences</key>
		<true/>
	</dict>
</array>
</plist>
